{
  "version": 2,
  "builds": [
      {
          "src": "server/app.js",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/api/(.*)",
          "dest": "/server/app.js",
          "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
          "headers": {
              "Access-Control-Allow-Origin": "*",
              "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
              "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept"
          }
      },
      {
          "src": "/",
          "dest": "/server/app.js",
          "methods": ["GET"]
      },
      {
          "src": "/(.*)",
          "dest": "/server/app.js"
      }
  ],
  "env": {
      "NODE_ENV": "production"
  },
  "regions": ["cdg1"]
}