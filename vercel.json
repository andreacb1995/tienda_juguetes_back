{
  "version": 2,
  "builds": [
      {
          "src": "server/app.js",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/api/(.*)",
          "dest": "/server/app.js",
          "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
      },
      {
          "src": "/",
          "dest": "/server/app.js",
          "methods": ["GET"]
      },
      {
          "src": "/(.*)",
          "dest": "/server/app.js"
      }
  ],
  "env": {
      "NODE_ENV": "production"
  },
  "regions": ["cdg1"]
}